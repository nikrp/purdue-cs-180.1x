/**
 * Auto Generated Java Class.
 */
public class Robot {
  void print(String text) {
    System.out.println(text);
  }
  
  String name;
  
  Robot(String name) {
    this.name = name;
  }
  
  void speak(String msg) {
    print(msg + " I'm " + name + "!");
  }
  
  public static void main(String[] args) {
    Robot r = new Robot("Nikhil");
    //r.name = "Nikhil";
    r.speak("Hello!");
  }
  


  /* ADD YOUR CODE HERE */
  
}
